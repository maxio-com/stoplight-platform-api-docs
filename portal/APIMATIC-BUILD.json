{
  "$schema": "https://titan.apimatic.io/api/build/schema",
  "buildFileVersion": "1",
  "generatePortal": {
    "debug": {
      "publishReport": true
    },
    "pageTitle": "Maxio Advanced Billing Developer Portal",
    "navTitle": "Advanced Billing Developer Portal",
    "logoUrl": "static/images/maxio_logo.png",
    "logoLink": "https://www.maxio.com/",
    "logoAltText": "Maxio Logo",
    "faviconUrl": "static/images/favicon.png",
    "useHostedPortalScript": true,
    "languageConfig": {
      "http": {},
      "csharp": {
        "packageRepository": "NuGet",
        "packageSettings": {
          "PackageName": "Maxio.AdvancedBillingSdk",
          "Version": "1.0.0"
        }
      },
      "java": {
        "packageRepository": "Maven",
        "packageSettings": {
          "GroupId": "com.maxio",
          "PackageName": "advanced-billing-sdk",
          "Version": "1.0.0"
        }
      },
      "php": {
        "packageRepository": "Packagist",
        "packageSettings": {
          "VendorName": "maxio",
          "PackageName": "advanced-billing-sdk",
          "Version": "1.0.0"
        }
      },
      "python": {
        "packageRepository": "PyPI",
        "packageSettings": {
          "PackageName": "maxio-advanced-billing-sdk",
          "Version": "1.0.0"
        }
      },
      "ruby": {
        "packageRepository": "RubyGems",
        "packageSettings": {
          "PackageName": "maxio-advanced-billing-sdk",
          "Version": "1.0.0"
        }
      },
      "typescript": {
        "packageRepository": "Npm",
        "packageSettings": {
          "PackageName": "@maxio-com/advanced-billing-sdk",
          "Version": "1.0.0"
        }
      },
      "go": {
        "packageSettings": {
          "PackageName": "github.com/maxio-com/ab-golang-sdk",
          "Version": "v1.0.0-alpha.1"
        }
      }
    },
    "portalSettings": {
      "baseUrl": "./",
      "enableExport": true,
      "enableConsoleCalls": true,
      "useProxyForConsoleCalls": false,
      "initialPlatform": "http_curl_v1",
      "languageSettings": {
        "http_curl_v1": {},
        "java_eclipse_jre_lib": {
          "disableSdkDownload": true
        },
        "cs_net_standard_lib": {
          "disableSdkDownload": true
        },
        "php_generic_lib_v2": {
          "disableSdkDownload": true
        },
        "python_generic_lib": {
          "disableSdkDownload": true
        },
        "ruby_generic_lib": {
          "disableSdkDownload": true
        },
        "ts_generic_lib": {
          "disableSdkDownload": true
        },
        "go_generic_lib": {
          "disableSdkDownload": true
        }
      },
      "themeOverrides": {
        "palette": {
          "primaryColor": "#0046FF",
          "linkColor": "#006EFE",
          "colors": {
            "C000": "#FFFFFF",
            "C200": "#FFFFFF",
            "C300": "#D9D9D9",
            "C400": "#B4B4B4",
            "C600": "#757575",
            "C700": "#757575",
            "C800": "#757575",
            "C900": "#000000"
          }
        },
        "fontSource": [
          "https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&display=swap"
        ],
        "cssStyles": {
          "headings": {
            "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
            "h1": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "1.5rem",
              "fontWeight": "600",
              "lineHeight": "2rem"
            },
            "h2": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "1.25rem",
              "fontWeight": "600",
              "lineHeight": "1.75rem"
            },
            "h3": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "1rem",
              "fontWeight": "600",
              "lineHeight": "1.5rem"
            },
            "h4": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "0.875rem",
              "fontWeight": "600",
              "lineHeight": "1.25rem"
            },
            "h5": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "0.875rem",
              "fontWeight": "600",
              "lineHeight": "1.25rem"
            },
            "h6": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "0.875rem",
              "fontWeight": "600",
              "lineHeight": "1.25rem"
            }
          },
          "body": {
            "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
            "text1": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "1rem",
              "fontWeight": "400",
              "lineHeight": "1.5rem"
            },
            "text2": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "0.875rem",
              "fontWeight": "400",
              "lineHeight": "1.25rem"
            },
            "text3": {
              "fontFamily": "'Inter', -apple-system, BlinkMacSystemFont, San Francisco, Segoe UI, Roboto, Helvetica Neue, sans-serif",
              "fontSize": "0.75rem",
              "fontWeight": "400",
              "lineHeight": "1.125rem"
            }
          },
          "code": {
            "fontFamily": "ui-monospace, SFMono-Regular, SF Mono, 'Segoe UI Mono', 'Roboto Mono', 'Ubuntu Mono', Menlo, Consolas, Courier, monospace",
            "inlineCode": {
              "fontFamily": "ui-monospace, SFMono-Regular, SF Mono, 'Segoe UI Mono', 'Roboto Mono', 'Ubuntu Mono', Menlo, Consolas, Courier, monospace",
              "fontSize": "0.875rem",
              "fontWeight": "400",
              "lineHeight": "1.25rem"
            },
            "blockCode": {
              "fontFamily": "ui-monospace, SFMono-Regular, SF Mono, 'Segoe UI Mono', 'Roboto Mono', 'Ubuntu Mono', Menlo, Consolas, Courier, monospace",
              "fontSize": "0.875rem",
              "fontWeight": "400",
              "lineHeight": "1.25rem"
            }
          }
        }
      }
    }
  }
}
